{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "uploads/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "uploads/account.jpg"
    },
    {
      "src": "/(.+)",
      "dest": "uploads/$1"
    }
  ]
}
